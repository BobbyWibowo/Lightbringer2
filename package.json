{
    "name": "lightbringer",
    "version": "2.0.0",
    "description": "Yet another Discord self-bot written with Discord.js",
    "author": "Bobby Wibowo",
    "license": "MIT",
    "repository": "github:BobbyWibowo/Lightbringer",
    "bugs": "https://github.com/BobbyWibowo/Lightbringer/issues",
    "main": "./src/bot.js",
    "scripts": {
        "setup": "node ./src/setup.js",
        "start": "node ./src/bot.js",
        "background": "screen -dmS lightbringer node ./src/bot.js",
        "pm2": "pm2 start --name lightbringer ./src/bot.js",
        "lint": "standard ./src",
        "update": "git pull && yarn install && yarn upgrade"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "dependencies": {
        "bufferutil": "^3.0.3",
        "chalk": "^2.3.0",
        "common-tags": "^1.5.1",
        "discord-akairo": "1Computer1/discord-akairo",
        "discord.js": "hydrabolt/discord.js",
        "fs-extra": "^4.0.2",
        "moment": "^2.19.2",
        "path": "^0.12.7",
        "readline": "^1.3.0",
        "snekfetch": "^3.5.8"
    },
    "devDependencies": {
        "standard": "^10.0.3"
    },
    "resolutions": {
        "discord.js/snekfetch": "^3.5.8"
    }
}
