{
  "name": "lightbringer2",
  "version": "2.0.0-dev",
  "description": "Yet another Discord self-bot written with Discord.js (and Discord Akairo framework)",
  "author": "Bobby Wibowo",
  "license": "MIT",
  "repository": "github:BobbyWibowo/Lightbringer2",
  "bugs": "https://github.com/BobbyWibowo/Lightbringer2/issues",
  "main": "./src/bot.js",
  "scripts": {
    "setup": "node ./src/setup.js",
    "start": "node ./src/bot.js",
    "background": "screen -dmS lb2 node ./src/bot.js",
    "pm2": "pm2 start --name lb2 ./src/bot.js",
    "lint": "standard",
    "update": "git pull && yarn install"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "booru": "^0.4.0",
    "bufferutil": "^3.0.3",
    "canvas": "^1.6.9",
    "chalk": "^2.3.2",
    "color-convert": "^1.9.1",
    "color-name": "^1.1.3",
    "common-tags": "^1.7.2",
    "discord-akairo": "1Computer1/discord-akairo#a15fad602c7d0f05683c8d6ff85eb411b65e9b96",
    "discord.js": "hydrabolt/discord.js#ef8366d189b8c53f4acdc279caaae9fe682d63f2",
    "encodeurl": "^1.0.2",
    "fs-extra": "^5.0.0",
    "mathjs": "^4.0.0",
    "moment": "^2.21.0",
    "mw-dict": "^2.2.0",
    "path": "^0.12.7",
    "pixel-average": "^1.0.1",
    "readline": "^1.3.0",
    "snekfetch": "^3.6.4",
    "txtgen": "^2.0.1",
    "urban-dictionary-es6": "^1.0.1"
  },
  "devDependencies": {
    "standard": "^11.0.0"
  },
  "resolutions": {
    "discord.js/snekfetch": "^3.6.4"
  }
}
