{
  "name": "lightbringer",
  "version": "2.0.0-dev",
  "description": "Yet another Discord self-bot written with Discord.js",
  "author": "Bobby Wibowo",
  "license": "MIT",
  "repository": "github:BobbyWibowo/Lightbringer",
  "bugs": "https://github.com/BobbyWibowo/Lightbringer/issues",
  "main": "./src/bot.js",
  "scripts": {
    "setup": "node ./src/setup.js",
    "start": "node ./src/bot.js",
    "background": "screen -dmS lightbringer node ./src/bot.js",
    "pm2": "pm2 start --name lightbringer ./src/bot.js",
    "lint": "standard",
    "update": "git pull && yarn install"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "booru": "BobbyWibowo/booru",
    "bufferutil": "^3.0.3",
    "chalk": "^2.3.0",
    "common-tags": "^1.7.2",
    "discord-akairo": "1Computer1/discord-akairo",
    "discord.js": "hydrabolt/discord.js",
    "encodeurl": "^1.0.2",
    "fs-extra": "^5.0.0",
    "mathjs": "^3.20.1",
    "moment": "^2.20.1",
    "mw-dict": "^2.2.0",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "snekfetch": "^3.6.1"
  },
  "devDependencies": {
    "standard": "^10.0.3"
  },
  "resolutions": {
    "discord.js/snekfetch": "^3.6.1"
  }
}
